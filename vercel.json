{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "env": {},
    "redirects": [
      {
        "source": "/live",
        "destination": "https://twitch.tv/lucasbernalte"
      }
    ],
    "headers": [
      {
        "source": "/api/now-playing",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=60, stale-while-revalidate=30"
          }
        ]
      }
    ]
  }